<p id="notice"><%= notice %></p>

<h1>Listing Guest Weddings</h1>

<table>
  <thead>
    <tr>
      <th>Wedding</th>
      <th>Guest</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @guest_weddings.each do |guest_wedding| %>
      <tr>
        <td><%= guest_wedding.wedding_id %></td>
        <td><%= guest_wedding.guest_id %></td>
        <td><%= link_to 'Show', guest_wedding %></td>
        <td><%= link_to 'Edit', edit_guest_wedding_path(guest_wedding) %></td>
        <td><%= link_to 'Destroy', guest_wedding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Guest wedding', new_guest_wedding_path %>
